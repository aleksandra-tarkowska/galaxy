<tool id="omero" name="OMERO" version="0.1.0">
  <description>load map</description>
  <command interpreter="python">
   load_from_omero.py 
      $image_id
  </command>
  <inputs>
    <param name="image_id" size="5" type="integer" value="1229801" label="Image id" help="lines"/>
  </inputs>
  <outputs>
    <data format="input" name="out_file1" metadata_source="input"/>
  </outputs>
  <tests>
    <test>
      <param name="image_id" value="1229801"/>
      <param name="input" value="1.bed"/>
      <output name="out_file1" file="eq-showbeginning.dat"/>
    </test>
  </tests>
